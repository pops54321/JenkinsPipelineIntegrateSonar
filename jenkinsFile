node{
    stages{
    stage('SCM Checkout'){
        steps{
            git 'https://github.com/pops54321/JenkinsPipelineIntegrateSonar'
        }
    }
    stage('Compile-Package'){
        steps{
           sh 'mvn clean install'
            echo "maven compiled the package"
        }
    }
    stage('SonarQube-analysis'){
        steps{
            sh 'sonar:sonar'
            echo "Sonar generated the report"
        }
    }
}
}
